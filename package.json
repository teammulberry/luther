{
  "name": "luther",
  "version": "0.1.0",
  "homepage": "https://teammulberry.github.io/luther",
  "private": true,
  "devDependencies": {
    "gh-pages": "^0.11.0",
    "react-scripts": "0.7.0"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-dragula": "^1.1.17",
    "react-router": "^3.0.0",
    "tachyons": "^4.5.5",
    "tachyons-cli": "^1.0.10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "css": "tachyons src/css/app.css > src/index.css --minify",
    "prestart": "npm run css",
